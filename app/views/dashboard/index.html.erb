<% if user_signed_in? %>

<h1>Welcome to your dashboard <%= current_user.full_name %>!</h1>

<% for survey in @surveys do %>
<%= link_to "#{survey.title}", survey_path(survey.id) %>
<br>
<% end %>
<%= link_to "Build your Profile", profile_path %>
<%= link_to "Log Out", destroy_user_session_path, method: :delete %>


<% else %>
<h1>You should not be here, 404?</h1>

<% end %>